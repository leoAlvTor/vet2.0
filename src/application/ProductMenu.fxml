<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControllerProductsMenu">
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="225.0" minHeight="10.0" prefHeight="123.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="131.0" minHeight="10.0" prefHeight="131.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="btnInventario" mnemonicParsing="false" onAction="#openInventario" prefHeight="130.0" prefWidth="170.0" text="INVENTARIO" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Button fx:id="btnCompras" mnemonicParsing="false" onAction="#openCompras" prefHeight="130.0" prefWidth="170.0" text="COMPRAS" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1">
               <children>
                  <Button fx:id="btnProveedor" mnemonicParsing="false" onAction="#openProveedor" prefHeight="130.0" prefWidth="170.0" text="PROVEEDORES" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
               <children>
                  <Button fx:id="btnProducto" mnemonicParsing="false" onAction="#openProductos" prefHeight="130.0" prefWidth="170.0" text="NUEVO PRODUCTO" />
               </children>
            </HBox>
            <Button fx:id="btnNotas" mnemonicParsing="false" onAction="#openNotas" prefHeight="25.0" prefWidth="300.0" text="NOTAS DE CREDITO" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
         <StackPane.margin>
            <Insets bottom="10.0" top="10.0" />
         </StackPane.margin>
      </GridPane>
   </children>
</StackPane>
